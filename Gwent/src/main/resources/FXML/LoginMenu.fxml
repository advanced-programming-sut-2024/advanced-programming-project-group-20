<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="900.0" stylesheets="@Register.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginMenu">

    <Button id="loginButton" layoutX="423.0" layoutY="392.0" onMouseClicked="#login" prefHeight="43.0" prefWidth="119.0" text="Login">
        <font>
            <Font size="15.0" />
        </font></Button>
    <VBox layoutX="330.0" layoutY="304.0" prefHeight="74.0" prefWidth="239.0" spacing="3">
        <TextField fx:id="usernameField" promptText="username" styleClass="text-field" />
        <TextField fx:id="passwordField" promptText="password" styleClass="text-field" />

    </VBox>
    <Label layoutX="7.0" layoutY="8.0" text="Login menu" />
    <Label layoutX="381.0" layoutY="260.0" text="we wait for you!" />
    <Button layoutX="240.0" layoutY="397.0" mnemonicParsing="false" onMouseClicked="#goRegisterMenu" prefHeight="30.0" prefWidth="159.0" text="go Back and register" />
</AnchorPane>

