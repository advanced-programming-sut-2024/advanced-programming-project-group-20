<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@Profile.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ProfileMenu">
    <children>
        <Pane fx:id="informationPane" layoutX="14.0" visible="false">
            <children>
            <VBox layoutX="494.0" layoutY="215.0" prefHeight="129.0" prefWidth="148.0" spacing="2" stylesheets="@Profile.css">
                <children>
                    <Label prefHeight="30.0" prefWidth="160.0" text="username:" />
                    <Label prefHeight="30.0" prefWidth="153.0" text="password:" />
                    <Label prefHeight="30.0" prefWidth="208.0" text="email:" />
                    <Label prefHeight="30.0" prefWidth="200.0" text="nickname:" />
                </children>
            </VBox>
            <VBox layoutX="658.0" layoutY="221.0" prefHeight="110.0" prefWidth="136.0" spacing="2">
                <children>
                    <TextField fx:id="username" />
                    <TextField fx:id="password" />
                    <TextField fx:id="email" />
                    <TextField fx:id="nickname" prefHeight="26.0" prefWidth="136.0" />
                </children>
            </VBox>
            <Label alignment="CENTER" layoutX="514.0" layoutY="109.0" prefHeight="56.0" prefWidth="260.0" text="your informations!" textAlignment="CENTER">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <VBox layoutX="542.0" layoutY="413.0" prefHeight="98.0" prefWidth="204.0" spacing="3" stylesheets="@Profile.css">
                <children>
                    <Label fx:id="topScoreLabel" prefHeight="30.0" prefWidth="208.0" text="top score:" />
                    <Label fx:id="rankLabel" prefHeight="30.0" prefWidth="203.0" text="rank:" />
                    <Label fx:id="totalGamesLabel" prefHeight="30.0" prefWidth="204.0" text="number of games:" />
                </children>
            </VBox>
            <VBox layoutX="608.0" layoutY="512.0" prefHeight="66.0" prefWidth="100.0" spacing="3">
                <children>
                    <Label fx:id="winLabel" prefHeight="94.0" prefWidth="100.0" text="win:" />
                    <Label fx:id="drawLabel" prefHeight="94.0" prefWidth="100.0" text="draw:" />
                    <Label fx:id="loseLabel" prefHeight="94.0" prefWidth="100.0" text="fail:" />
                </children>
            </VBox>
            <Button layoutX="578.0" layoutY="360.0" mnemonicParsing="false" onMouseClicked="#saveChanges" prefHeight="26.0" prefWidth="131.0" text="save changes" />
            </children>
        </Pane>
        <Button layoutX="43.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#BackToMainMenu" prefHeight="26.0" prefWidth="116.0" text="Back" />
        <ScrollPane fx:id="scrollOfHistory" layoutX="16.0" layoutY="66.0" prefHeight="556.0" prefWidth="1252.0" stylesheets="Profile.css" visible="false">
        </ScrollPane>
        <HBox layoutX="477.0" layoutY="641.0" prefHeight="37.0" prefWidth="340.0">
            <Button fx:id="button1" onAction="#button1Clicked" prefHeight="35.0" prefWidth="119.0" text="points rank" />
            <Button fx:id="button2" onAction="#button2Clicked" prefHeight="34.0" prefWidth="115.0" text="History" />
            <Button fx:id="button3" onAction="#button3Clicked" prefHeight="34.0" prefWidth="106.0" scaleX="1.2" scaleY="1.2" text="informatios" />
        </HBox>
      <ScrollPane fx:id="scrollOfPointsTable" layoutX="394.0" layoutY="140.0" prefHeight="463.0" prefWidth="506.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>


    </children>
</AnchorPane>
